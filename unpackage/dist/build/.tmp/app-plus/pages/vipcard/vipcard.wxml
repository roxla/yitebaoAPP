<view id="VIPCARD"><view class="vipcard-box"><scroll-view style="{{('height:'+height+'px')}}" scroll-y><view class="vipcard-item"><block wx:for="{{cardList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectType',['$0',index],[[['cardList','',index]]]]]]]}}" class="{{['card-box',cardActive==index?'box':'']}}" bindtap="__e"><view class="card-box-item"><view data-event-opts="{{[['tap',[['goRouter',['$0'],[[['cardList','',index]]]]]]]}}" class="{{['card-explain',cardActive==index?'explain':'']}}" bindtap="__e">本卡类型介绍</view><view class="card-money"><view>￥<text class="card-money-item">{{item.givePoint}}</text></view></view><view class="card-type"><view class="card-type-left">{{item.name}}</view><view class="card-type-right">{{item.discount+"折"}}</view></view></view></view></block></view></scroll-view></view><view class="wxpay"><button class="wxpay-button" style="background-color:#00CB15;" type="primary" data-event-opts="{{[['tap',[['submitCardWX']]]]}}" bindtap="__e"><view class="pay-box"><view class="pay-item-left"><image class="pay-img" src="../../static/vip_card/u606.png"></image><view>微信支付</view></view><view class="pay-item-right">￥<text class="pay-money">{{money}}</text></view></view></button></view><view class="alipay"><button class="alipay-button" style="background-color:#0F79FC;" type="primary"><view class="pay-box"><view class="pay-item-left"><image class="pay-img" src="../../static/vip_card/u608.png"></image><view>支付宝支付</view></view><view class="pay-item-right">￥<text class="pay-money">{{money}}</text></view></view></button></view><system vue-id="1" bind:__l="__l"></system></view>