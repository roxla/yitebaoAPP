<view id="FEEDBACK" class="data-v-7bce84ec"><view data-event-opts="{{[['tap',[['toggleTab',['selector']]]]]}}" class="feedback_type_box data-v-7bce84ec" bindtap="__e"><view class="feedback_type data-v-7bce84ec">{{type}}</view><uni-icon class="uni_Icon data-v-7bce84ec" vue-id="1" type="arrowright" color="#999" size="20" bind:__l="__l"></uni-icon></view><view class="feedback_text_box data-v-7bce84ec"><textarea class="feedback_text data-v-7bce84ec" maxlength="300" placeholder="请输入反馈信息" data-event-opts="{{[['input',[['__set_model',['','feedText','$event',[]]]]]]}}" value="{{feedText}}" bindinput="__e"></textarea><view class="feedback_text_tip data-v-7bce84ec">{{"已输入"+textNum+"/300"}}</view></view><view class="feedback_tel_box data-v-7bce84ec"><input class="userName data-v-7bce84ec" type="text" placeholder="手机/邮箱/QQ(选填)"/></view><view class="feedback_img_box data-v-7bce84ec"><view class="img_text_box data-v-7bce84ec"><view class="data-v-7bce84ec">图片上传</view><view class="data-v-7bce84ec">{{imgList.length+"/4"}}</view></view><view class="feedback_img data-v-7bce84ec"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="img_box data-v-7bce84ec" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['viewImage',['$event']]]]]}}" bindtap="__e"><image class="feed_img data-v-7bce84ec" src="{{imgList[index]}}" mode="aspectFill"></image><view class="close_box data-v-7bce84ec" data-index="{{index}}" data-event-opts="{{[['tap',[['delImg',['$event']]]]]}}" catchtap="__e"><uni-icon class="uni_close data-v-7bce84ec" vue-id="{{'2-'+index}}" type="closeempty" color="#FFFFFF" size="20" bind:__l="__l"></uni-icon></view></view></block><block wx:if="{{imgList.length<4}}"><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="img_blank data-v-7bce84ec" bindtap="__e"><uni-icon class="uni_Icon data-v-7bce84ec" vue-id="3" type="camera" color="#999" size="30" bind:__l="__l"></uni-icon></view></block></view></view><view class="login_button data-v-7bce84ec"><button class="button_color data-v-7bce84ec" style="background-color:#02ABA4;" type="primary" data-event-opts="{{[['tap',[['upData',['$event']]]]]}}" bindtap="__e">提交</button></view><block wx:if="{{selectList.length!=0}}"><w-picker vue-id="4" mode="selector" defaultVal="{{[0]}}" themeColor="#3699FC" selectList="{{selectList}}" data-ref="selector" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e" class="data-v-7bce84ec vue-ref" bind:__l="__l"></w-picker></block></view>