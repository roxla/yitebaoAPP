<view id="REG"><view class="reg_box"><view class="reg_logo"><avatar vue-id="1" selWidth="200px" selHeight="400upx" avatarSrc="{{userImg}}" noTab="{{true}}" avatarStyle="width: 100%;height: 100%;position: absolute;left: 0;border-radius: 100%;" data-event-opts="{{[['^upload',[['myUpload']]]]}}" bind:upload="__e" bind:__l="__l"></avatar></view><view class="reg_input_box"><view class="reg_input"><view class="icon_box"><uni-icon class="uni_Icon" vue-id="2" type="phone-filled" color="#999" size="25" bind:__l="__l"></uni-icon></view><view class="uni_input"><input class="Name" placeholder="请输入注册手机号码" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view></view><view class="reg_input reg_mail"><view class="icon_box"><uni-icon class="uni_Icon" vue-id="3" type="email-filled" color="#999" size="25" bind:__l="__l"></uni-icon></view><view class="uni_input"><input class="Name" placeholder="请输入短信验证码" data-event-opts="{{[['input',[['__set_model',['','res','$event',[]]]]]]}}" value="{{res}}" bindinput="__e"/></view><view class="uni_button"><button type="primary" disabled="{{show}}" plain="true" size="mini" data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" bindtap="__e">{{codeName}}</button></view></view><view class="reg_input"><view class="icon_box"><uni-icon class="uni_Icon" vue-id="4" type="locked" color="#999" size="25" bind:__l="__l"></uni-icon></view><view class="uni_input"><input class="Name" placeholder="请设置6-20位新的登录密码" password="{{true}}" data-event-opts="{{[['input',[['__set_model',['','pwd','$event',[]]]]]]}}" value="{{pwd}}" bindinput="__e"/></view></view><view class="reg_input"><view class="icon_box"><uni-icon class="uni_Icon" vue-id="5" type="locked" color="#999" size="25" bind:__l="__l"></uni-icon></view><view class="uni_input"><input class="Name" placeholder="请再次输入新的登录密码" password="{{true}}" data-event-opts="{{[['input',[['__set_model',['','repwd','$event',[]]]]]]}}" value="{{repwd}}" bindinput="__e"/></view></view></view><view class="reg_rule"><label class="radio"><radio data-event-opts="{{[['tap',[['ruleAccount']]]]}}" style="transform:scale(0.8);" bindtap="__e"></radio></label><view>同意《<navigator class="reg_rule_item" url="../rule/rule">用户服务协议</navigator>》</view></view><view class="reg_button"><button class="button_color" style="background-color:#02ABA4;" type="primary" disabled="{{reg}}" data-event-opts="{{[['tap',[['regAccount',['$event']]]]]}}" bindtap="__e">注册</button></view><view class="reg_link"><navigator class="reg_link_item" open-type="reLaunch" url="{{loginUrl}}">已有账户，立即登录</navigator></view></view></view>